@using Bills.Models;
@model List<Session>

<table class="table">
    <thead>
        <tr>
            <th>
                SessionId
            </th>
            <th>
                Number
            </th>
            <th>
                StartDate
            </th>
            <th>
                EndDate
            </th>
            <th>
                CommonsDescription
            </th>
            <th>
                LordsDescription
            </th>
            <th>
                SessionNumber
            </th>
            <th>
                ParliamentNumber
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SessionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CommonsDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LordsDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SessionNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParliamentNumber)
            </td>

        </tr>
}
    </tbody>
</table>
